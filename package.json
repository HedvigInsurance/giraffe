{
  "name": "giraffe",
  "main": "dist/index.js",
  "scripts": {
    "start": "node .",
    "dev": "ts-node src/index.ts",
    "build": "tsc && cp src/schema.graphqls dist/schema.graphqls",
    "watch": "concurrently -n \"ts-node,graphqls\" \"nodemon -e ts -w ./src -x yarn dev\" \"nodemon -e graphqls -w ./src -x yarn generate:schema-types\"",
    "lint": "tslint **/*.ts",
    "generate:schema-types": "ts-node scripts/generate-types.ts"
  },
  "license": "AGPL-3.0-only",
  "dependencies": {
    "@google-cloud/logging": "^5.1.3",
    "@sentry/node": "^5.5.0",
    "apollo-link": "^1.2.12",
    "apollo-link-context": "^1.0.18",
    "apollo-link-http": "^1.5.15",
    "apollo-link-redis-cache": "0.0.3",
    "apollo-server-core": "^2.6.8",
    "apollo-server-koa": "^2.6.8",
    "aws-sdk": "^2.495.0",
    "date-fns": "^1.30.1",
    "dotenv": "^8.0.0",
    "graphql": "^14.4.2",
    "graphql-middleware": "^3.0.2",
    "graphql-redis-subscriptions": "^2.1.0",
    "graphql-subscriptions": "1.1.0",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.5",
    "ioredis": "^4.11.1",
    "koa": "^2.7.0",
    "koa-better-http-proxy": "^0.2.4",
    "koa-compress": "^3.0.0",
    "koa-route": "^3.2.0",
    "node-fetch": "^2.6.0",
    "ramda": "^0.26.1",
    "subscriptions-transport-ws": "^0.9.16",
    "typescript-logging": "^0.6.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/koa-compress": "^2.0.9",
    "@types/koa-route": "^3.2.4",
    "@types/ramda": "^0.26.15",
    "@types/dotenv": "^6.1.1",
    "@types/graphql": "^14.2.2",
    "@types/ioredis": "^4.0.13",
    "@types/koa": "^2.0.49",
    "@types/node-fetch": "2.3.7",
    "@types/uuid": "^3.4.5",
    "@types/ws": "^6.0.1",
    "apollo": "^2.15.0",
    "concurrently": "^4.1.1",
    "graphql-schema-typescript": "^1.2.9",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "ts-node": "^8.0.3",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.3"
  },
  "resolutions": {
    "event-stream": "4.0.1"
  }
}
